DECLARE
 P_VARCHAR01 VARCHAR2(50);
 P_VARCHAR02 CONSTANT VARCHAR2(50) := 'XXX'; /*ตัวแปรเปลี่ยนค่าไม่ได้*/
 P_INT01 NUMBER(6);
BEGIN
 SELECT 'AA' AS VC01, 50 AS NUM01 INTO P_VARCHAR01,P_INT01
 FROM DUAL;
 DBMS_OUTPUT.PUT_LINE('VC01: ' || P_VARCHAR01);
 DBMS_OUTPUT.PUT_LINE('vc02: ' || P_VARCHAR02);
 DBMS_OUTPUT.PUT_LINE('INT: ' || P_INT01);
END;
 
